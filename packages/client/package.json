{
  "name": "@sb-prisma/client",
  "description": "Automatically generates database code for supabse-js from prisma schema.",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.27",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.3"
  },
  "homepage": "https://github.com/aiji42/prisma-generator-supabase",
  "repository": {
    "type": "git",
    "url": "https://github.com/aiji42/prisma-generator-supabase"
  },
  "author": "Aiji Uejima <aiji42@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
